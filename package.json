{
  "name": "@angular2/electron",
  "version": "1.0.0",
  "description": "Electron Platform for Angular2",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "yarn run build:src && yarn run bundle && yarn run demo",
    "build:src": "ngc",
    "build:demo": "cp -r modules/demo-app/*.{html,json} dist/demo-app",
    "bundle": "webpack",
    "demo": "yarn run build:demo && electron dist/demo-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular/angular-electron.git"
  },
  "keywords": [
    "Angular2",
    "Electron"
  ],
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular/angular-electron/issues"
  },
  "homepage": "https://github.com/angular/angular-electron#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.37.2",
    "typescript": "^1.8.9",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "angular2": "2.0.0-beta.12"
  },
  "dependencies": {
    "parse5": "^1.5.1"
  }
}
